{
  "name": "learnproject3",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "dev": "set NODE_PATH=./lib&& nodemon --exec ts-node lib/server.ts  --ignore public/",
    "devAuth": "set NODE_PATH=./lib&& nodemon --exec ts-node lib/authServer.ts  --ignore public/",
    "webpack": "webpack -w --mode development"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "@typescript-eslint/eslint-plugin"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      },
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    }
  },
  "author": "",
  "license": "ISC",
